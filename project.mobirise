                  "transition": "all 0.3s ease-in-out",
                  "cursor": "pointer",
                  "&:hover, &:focus": {
                    "color": "@active"
                  },
                  "&:last-child": {
                    "margin-bottom": "0"
                  }
                }
              }
            },
            ".text-wrapper": {
              "margin-bottom": "48px",
              "@media (max-width: 992px)": {
                "margin-bottom": "24px"
              },
              ".mbr-desc": {
                "margin-bottom": "16px"
              },
              ".mbr-text": {
                "margin-bottom": "16px"
              }
            },
            ".mbr-copy": {
              "margin-bottom": "0",
              "color": "#5f6368",
              "text-align": "center"
            },
            ".mbr-desc": {
              "color": "#ffffff",
              "text-align": "center"
            },
            ".mbr-text, .text-wrapper": {
              "color": "#9aa0a6"
            },
            ".list": {
              "color": "#9aa0a6"
            },
            ".mbr-text, .mbr-section-btn": {
              "text-align": "center"
            },
            ".list, .item-wrap": {
              "text-align": "left",
              "color": "#378ffd"
            },
            ".mbr-desc, .content-wrapper": {
              "text-align": "left"
            }
